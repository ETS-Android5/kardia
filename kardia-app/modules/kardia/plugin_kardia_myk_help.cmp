$Version=2$
plugin_kardia_myk_help "widget/component-decl"
    {
    width=390; height=32;
    description = "Help";

    help_hbox "widget/hbox"
	{
	x=0; y=0; width=390; height=32;
	spacing=12;
	help_img "widget/image" { width=32; source="/apps/kardia/images/tango/help-browser.png"; }
	help_label "widget/label" { widget_class=link; y=4; width=300; font_size=20; style=bold; text="Help"; }
	}
    }
